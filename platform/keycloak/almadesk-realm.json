  "roles": {
    "realm": [
      {
        "name": "USER",
        "description": "Regular user - can create and view own tickets",
        "composite": false,
        "clientRole": false
      },
      {
        "name": "AGENT",
        "description": "IT Agent - can manage all tickets",
        "composite": false,
        "clientRole": false
      },
      {
        "name": "SUPER_ADMIN",
        "description": "Super Administrator - full access to system",
        "composite": false,
        "clientRole": false
      }
    ]
  },
  "groups": [
    {
      "name": "students",
      "path": "/students",
      "attributes": {},
      "realmRoles": ["USER"]
    },
    {
      "name": "staff",
      "path": "/staff",
      "attributes": {},
      "realmRoles": ["USER"]
    },
    {
      "name": "it-support",
      "path": "/it-support",
      "attributes": {},
      "realmRoles": ["AGENT"]
    },
    {
      "name": "administrators",
      "path": "/administrators",
      "attributes": {},
      "realmRoles": ["SUPER_ADMIN"]
    }
  ],
  "users": [
